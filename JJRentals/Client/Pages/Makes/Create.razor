@page "/manus/create/"
@inject HttpClient _client
@inject NavigationManager _navManager

<h3 class="card-title">Create New Manufacturer</h3>
<hr />
<br />

<FormComponent ButtonText="Create Manufacturer" OnValidSubmit="CreateMake" manu="manu" />


@code {
    Manufacturer manu = new Manufacturer();

    private async Task CreateMake()
    {
        await _client.PostAsJsonAsync(Endpoints.MakesEndpoint, manu);
        _navManager.NavigateTo("/manus/");
    }

}